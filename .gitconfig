[alias]
  gethash = log -1 --pretty=format:%h
	co = checkout
	br = branch
	ci = commit
	st = status
	last = log -1 HEAD
	df = diff --color-words
  lg = log --pretty=format:'%Credcommit %H\n%CgreenAuthor: %an <%ae>\n%C(white)%ad (%ar)\n%C(cyan)%s\n' -3
  graph = log --graph --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold white)â€” %an%C(reset)%C(bold red)%d%C(reset)' --abbrev-commit --date=relative
  logdate = log --pretty=format:'%Cred%ar: %Cgreen %s'
